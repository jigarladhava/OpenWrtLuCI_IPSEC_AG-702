config vpn 'main'
	option enabled '0'
	option name 'site-to-site'
	option remote_ip ''
	option remote_subnet ''
	option local_subnet '192.168.1.0/24'
	option psk ''
	option auto_start '1'

config ike 'phase1'
	option version '1'
	option mode 'main'
	option encryption 'aes256'
	option hash 'sha2_256'
	option dh_group 'modp2048'
	option lifetime '5400'
	option rekey_fuzz '50'

config esp 'phase2'
	option encryption 'aes256'
	option hash 'sha2_256'
	option dh_group 'modp2048'
	option lifetime '3600'

config dpd 'deadpeer'
	option delay '30'
	option timeout '120'
	option action 'restart'

config options 'settings'
	option keyingtries 'forever'
	option uniqueids '1'
	option debug '0'

config ping_monitor 'health'
	option enabled '0'
	option local_ip ''
	option remote_primary '10.0.0.1'
	option remote_secondary '10.0.0.2'
	option initial_wait '10'
	option interval '1800'
	option retry_interval '5'
	option timeout '3'
	option max_tries '3'
